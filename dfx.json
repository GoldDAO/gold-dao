{
	"__0": "GLDT.org static website with swap canister",
	"canisters": {
		"gldt_frontend": {
			"type": "assets",
			"source": [
				"client/out"
			],
			"build": [
				"npm run postinstall",
				"npm run build:front"
			],
			"frontend": {}
		},
